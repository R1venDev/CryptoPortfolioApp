<VBox prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.cryptoapp.controllers.AssetsPriceNowController">

    <Label text="Введите цену актива:"/>

    <TextField fx:id="assetsPriceNowField"/>

    <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonClick" text="Сохранить"/>

</VBox>


